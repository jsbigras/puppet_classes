<?xml version="1.0"?>
<configuration>
    <http>
        <listening-interface><%= ipaddress %></listening-interface>
        <listening-port><%= l_port_abimport_mo %></listening-port>
    </http>
    
    <authentication>
        <!-- Supported values NONE, BASIC;  To disable a type simply comment it out... -->
        <scheme type="NONE" />
        
        <!--
        <scheme type="BASIC">
	        <param name="username" value="myUser" />
	        <param name="password" value="myPass" />
        </scheme>    
        -->
    </authentication>
    
    <impsClient>
        <http-client>
            <!-- 300000 == 5 minutes -both values has to be set to the same because of Ning -->
            <request-timeout-ms>300000</request-timeout-ms>
            <read-timeout-ms>300000</read-timeout-ms>
        </http-client>
    </impsClient>
    
    <normalization>
        <msisdn-normalization>
            <enabled>true</enabled>
            <!-- comma separated list of ISO-3166-1 -->
            <no-area-code-padding-csv>AR</no-area-code-padding-csv>
        </msisdn-normalization>
    </normalization>
    
    <handler>
        <impsUrl>http://<%= int_vip_cluster %>:<%= v_port_imps_be %>/userprovisioning/services/UserProvisioning</impsUrl>
    </handler>
</configuration>
