# The following keys and URI are deployment specific
yahooDirectOAuthSharedSecret=<%= yahoodirectoauthsharedsecret %>
yahooPwtokenSharedSecret=<%= yahoopwtokensharedsecret %>
yahooPartnerId=<%= yahoopartnerid %>
yahooConsumerKey=<%= yahooconsumerkey %>
yahooSubscriptionSharedSecret=<%= yahoosubscriptionsharedsecret %>
yahooNotificationSharedSecret=<%= yahoonotificationsharedsecret %>
yahooSubscribeUri=<%= yahoosubscribeuri %>

# The following URIs and config are common to all deployments
yahooPARTAuthUri = https://login.yahoo.com/WSLogin/V1/get_auth_token
yahooDirectOAuthUri = https://api.login.yahoo.com/oauth/v2/get_token
yahooPwtokenGetAuthUri = https://login.yahoo.com/config/pwtoken_get
yahooPwtokenLoginAuthUri = https://login.yahoo.com/config/pwtoken_login
yahooUri = http://social.yahooapis.com/v1/user/
yahooRealm = yahooapis.com
yahooOAuthVersion = 1.0


imap.uri.nonGenericOverride=<%= yahoo_imapurinongenericoverride %>
imap.auth.nonGenericOverride = XYMCOOKIEB64
smtp.uri.nonGenericOverride=<%= yahoo_smtpurinongenericoverride %>
smtp.auth.nonGenericOverride = XYMCOOKIE

# clients used by this plugin: first client IDs (will be used for OAM name) as CSV, then class name for each ID.
clients=default
clientType.default=com.nokia.nmp.httpclient.Client

# copy from imap:


system.mailboxes=Inbox=inbox,Sent=sent,Trash=trash,Draft=draft,Bulk Mail=spam
folder.management=true
root.level.folder.creation=false
child.folder.creation.disable=false

imap.search.supported=true
imap.search.criteria=UNDELETED

#imap.attachment.forward.maxsizekb=15360
attachment.send.maxuploadcount=10
attachment.total.maxsizekb=6144

smtp.transaction.timeoutSecond=30
smtp.ehloDomain=myriadgroup.com
smtp.encoding.charset=UTF-8
smtp.trailerLine.placement=middle
smtp.trailerLine=
