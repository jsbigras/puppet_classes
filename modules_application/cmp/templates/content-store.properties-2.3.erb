# content store host and application context
content.store.base.uri=http://<%= int_vip_cluster %>:<%= v_port_cstore_admin %>/store

# relative path for product resource
content.store.product.relative.path=/admin/product/{id}

# relative, templated, path for locale resource
content.store.locale.relative.path=/admin/product/{id}/locale/{locale}

# relative path for product purchasable flag
content.store.product.purchasable.flag.relative.path=/admin/product/{id}/platform/{platform}/purchasable/{purchasable}

# relative path for product content
content.store.product.platform.relative.path=/admin/product/{id}/platform/{platform}/size/{size}/content

# relative path for product preview
content.store.product.preview.relative.path=/admin/product/{id}/platform/{platform}/size/{size}/preview

# relative path for product thumbnail
content.store.product.thumbnail.relative.path=/admin/product/{id}/size/{size}/thumbnail

# relative path for product locale
content.store.product.locale.relative.path=/admin/product/{id}/locale/{locale}

# relative path for provider
content.store.provider.relative.path=/admin/provider

# aaa host and port
aaa.base.uri=http://<%= int_vip_cluster %>:<%= v_port_aaa_admin %>

# aaa relative path for get product 
aaa.get.product.path=/rps/3/billing/product

aaa.networkcode.header.name=x-unity-networkcode

domains.max.page.size=30
domains.min.page.size=1

# the version of the lawful interception portal
content.store.portal.version=1.1.0

