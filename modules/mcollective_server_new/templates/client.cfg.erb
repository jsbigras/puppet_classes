topicprefix = /topic/
main_collective = mcollective
collectives = mcollective
libdir = /usr/libexec/mcollective
logfile = /dev/null
loglevel = info

# Plugins
securityprovider = psk
plugin.psk = unset

connector = stomp
plugin.stomp.host = <%= mc_server %>
plugin.stomp.port = 6163
plugin.stomp.user = <%= mc_user %>
plugin.stomp.password = <%= mc_password %>


# Facts
factsource = yaml
plugin.yaml = /etc/mcollective/facts.yaml
