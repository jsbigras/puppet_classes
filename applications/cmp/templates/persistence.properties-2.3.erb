db.driverClass=com.mysql.jdbc.Driver
# format jdbc:mysql://[hostname or ip]:[port]/[database name]
db.url=jdbc:mysql://<%= db_vip %>:3306/<%= sch_sql_col_tbl.at(0) %>
db.username=<%= sch_sql_col_tbl.at(0) %>
db.password=<%= sch_pwd %>

hibernate.dialect=org.hibernate.dialect.MySQL5InnoDBDialect
hibernate.show_sql=false
hibernate.hbm2ddl.auto=update
