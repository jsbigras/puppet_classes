<?xml version="1.0"?>
<CONFIGURATION service="notification">
    <notification>
        <notificationServerHostPort><%= int_vip_cluster %>:<%= v_port_ns_http %></notificationServerHostPort>
        <channelCache>
            <id>channel</id>
            <ttlSec>1209600</ttlSec>
            <!-- 14 days -->
            <!-- list of comma separated channel types that cache state win over setChannels and which are not removable with removeChannel-->
            <cacheWinChannelTypes>tcp</cacheWinChannelTypes>
        </channelCache>
    </notification>
</CONFIGURATION>
